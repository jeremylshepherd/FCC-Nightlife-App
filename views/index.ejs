<!DOCTYPE html>
<html>

  <head>

    <title>Jeremy L. Shepherd | Universal React Template</title>
     
    <link type="text/css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
    <link type="text/css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css">
    <link type="text/css" rel="stylesheet" href="dist/app.css">
  </head>
    <body>
      <% if(messages.logout) {%>
      <div class="alert alert-success" role="alert"><span class="fa fa-thumbs-o-up"></span><%= messages.logout%></div>
      <% } %>
      <% if(messages.login) {%>
      <div class="alert alert-danger" role="alert"><span class="fa fa-hand-stop-o"></span><%= messages.login%></div>
      <% } %>
      <div id="app"><%-reactHTML%></div>
      <script type="text/javascript" src="dist/bundle.js"></script>
    </body>

</html>